local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Ten Nights Hood Hub", "DarkTheme")

-- MAIN
local Main = Window:NewTab("Main")
local MainSection = Main:NewSection("Main")


MainSection:NewButton("SilentAim", "An op streamable Silent Aim", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/kenji412/TenNights/main/TenNightsScript"))()
end)


MainSection:NewButton("CamLock", "An op Cam lock glitches when target jumps", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/kenji412/FlipFlopsCam/main/FlipsCamSource"))()
end)
